{
  "name": "hikes",
  "description": "A blog of photos from the most memorable hikes Kylie Stewart has been on",
  "version": "1.1.0",
  "author": "Kylie Stewart <kylie@kyliestewart.tech>",
  "license": "MIT",
  "homepage": "https://kylieis.online/hikes",
  "scripts": {
    "clean": "rm -rf public",
    "build": "yarn run clean && react-static build",
    "deploy": "yarn run build && gh-pages -d public",
    "dev": "react-static start",
    "format": "prettier --write --config .prettierrc \"**/*.{js,css,json,mdx,md}\" --ignore-path .prettierignore"
  },
  "dependencies": {
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-static": "^7.2.3"
  },
  "devDependencies": {
    "gh-pages": "^2.2.0",
    "prettier": "^2.0.4"
  }
}
